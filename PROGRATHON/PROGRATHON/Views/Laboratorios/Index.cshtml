@model IEnumerable<PROGRATHON.Models.Laboratorio>

<h2>Laboratorio</h2>
<a name=""
   id=""
   class="btn btn-primary"
   asp-action="Crear"
   role="button">Crear</a>


<div class="table-responsive">
    <table class="table table-primary">
        <thead>
            <tr>
                <th scope="col">IDLab</th>
                <th scope="col">NombreLab</th>
                <th scope="col">Capacidad</th>
                <th scope="col">Responsable</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Laboratorio itemDato in Model)
            {
                @* tr=fila *@
                <tr class="">
                    @* td= columnas *@
                    <td>@itemDato.IDLab</td>
                    <td>@itemDato.NombreLab</td>
                    <td>@itemDato.Capacidad</td>
                    <td>@itemDato.Responsable</td>
                </tr>
            }

            @*    <tr class="">
